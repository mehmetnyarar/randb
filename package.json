{
  "name": "randb-manager",
  "version": "1.0.0",
  "license": "",
  "description": "RAN database manager",
  "keywords": [
    "ran",
    "radio access network",
    "database",
    "manager"
  ],
  "author": "Mehmet N. Yarar <mehmetnyarar@gmail.com>",
  "contributors": [
    "Mehmet N. Yarar <mehmetnyarar@gmail.com>"
  ],
  "homepage": "",
  "repository": {},
  "bugs": {},
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "reset": "yarn clean && yarn install && npm-run-all build:logic build:ui",
    "clean": "lerna run clean --stream && rm -rf node_modules yarn*",
    "validate": "lerna run validate --stream --concurrency 1",
    "typings": "lerna run typings --stream --concurrency 1",
    "format": "npm-run-all format:root format:packages",
    "format:root": "prettier --write \"./*.{js,ts,json,html,css,md}\" --ignore-path .gitignore",
    "format:packages": "lerna run format --stream --concurrency 1",
    "lint": "npm-run-all lint:root lint:packages",
    "lint:root": "eslint --fix \"./*.{js,ts}\" --ignore-path .gitignore",
    "lint:packages": "lerna run lint --stream --concurrency 1",
    "test": "jest && lerna run test --stream --concurrency 1",
    "test:coverage": "jest && lerna run test --stream --concurrency 1 -- --coverage",
    "precommit": "lint-staged && lerna run precommit --stream --concurrency 1",
    "commit": "git add . && git-cz",
    "dev:server": "yarn --cwd packages/server dev",
    "test:server": "yarn --cwd packages/server test",
    "start:server": "yarn --cwd packages/server start",
    "apollo:service:push": "yarn --cwd packages/server apollo:push",
    "generate-gql": "yarn --cwd packages/shared-logic generate-gql",
    "dev:logic": "yarn --cwd packages/shared-logic dev",
    "test:logic": "yarn --cwd packages/shared-logic test",
    "build:logic": "yarn --cwd packages/shared-logic build",
    "apollo:client:check": "yarn --cwd packages/shared-logic apollo:check",
    "dev:ui": "yarn --cwd packages/shared-ui dev",
    "test:ui": "yarn --cwd packages/shared-ui test",
    "build:ui": "yarn --cwd packages/shared-ui build",
    "dev:web": "yarn --cwd packages/app-web dev",
    "test:web": "yarn --cwd packages/app-web test",
    "build:web": "yarn --cwd packages/app-web build",
    "start:web": "yarn --cwd packages/app-web start",
    "dev:mobile": "yarn --cwd packages/app-mobile dev",
    "test:mobile": "yarn --cwd packages/app-mobile test",
    "start:mobile": "yarn --cwd packages/app-mobile start"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/config-lerna-scopes": "^11.0.0",
    "@testing-library/dom": "^7.24.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/react-native": "^7.0.2",
    "@testing-library/user-event": "^12.1.7",
    "@types/jest": "^26.0.14",
    "@types/jest-axe": "^3.5.0",
    "@types/node": "^14.11.5",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "apollo": "^2.31.0",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.3.0",
    "jest": "^26.5.2",
    "jest-axe": "^4.0.0",
    "jest-expo": "^39.0.0",
    "jest-localstorage-mock": "^2.4.3",
    "jest-runner-eslint": "^0.10.0",
    "lerna": "^3.22.1",
    "lerna-update-wizard": "^0.17.8",
    "lerna-wizard": "^1.1.1",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-test-renderer": "16.13.1",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
